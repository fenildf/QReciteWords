<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="WordLableView">
        <attr name="WordLableTextContent" format="string" />
        <attr name="WordLableNumText" format="string" />
        <attr name="WordLableNumbg" format="reference|color" />
        <attr name="WordLableNumTxColor" format="color"/>
        <attr name="WordLableNumTxSize" format="dimension"/>
        <attr name="WordLableTxColor" format="color"/>
        <attr name="WordLableTxSize" format="dimension"/>
        <attr name="WordLableVisibility" format="integer"/>
    </declare-styleable>

    <declare-styleable name="AnimDownloadProgressButton">
        <attr name="progressbtn_radius" format="float" />
        <attr name="progressbtn_backgroud_color" format="color" />
        <attr name="progressbtn_backgroud_second_color" format="color" />
        <attr name="progressbtn_text_color" format="color" />
        <attr name="progressbtn_text_covercolor" format="color" />
    </declare-styleable>

    <declare-styleable name="RangeSliderView">
        <attr name="rangeCount" format="integer"/>
        <attr name="filledColor" format="color"/>
        <attr name="emptyColor" format="color"/>
        <attr name="barHeightPercent" format="float"/>
        <attr name="slotRadiusPercent" format="float"/>
        <attr name="sliderRadiusPercent" format="float"/>
    </declare-styleable>

</resources>